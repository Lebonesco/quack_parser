/*
* An overriding method can have argument types
* that are the same or super classes of the method
* they inherit and return subtypes.
*	
*/

class C1() {
	// nothing
}

class C2() extends C1 {
	def method1(val : C2) {
		// do method
	}
}

class C3() extends C2 {
	def method1(val : C1) {
		// do method
	}
}